{    "id": "http://ndm.mit.edu/ndm/schemas/runDay",
     "$schema": "http://json-schema.org/draft-06/schema#",
     "title": "runDay",
     "description": "Experiment Run Day Schema",
     "definitions": {},
     "type": "object",
     "allOf": [
	 { "$ref": "_NDMobject" } ],
     "properties": {
         "operators": {
             "type": "STRING",
             "defaultvalue":"Bent"
         },
         "shortDescription": {
             "type": "STRING",
             "defaultvalue":"'What we are doing today'"
         },
	 "runDate": {
	     "type": "STRING",
	     "format": "date-time",
	     "defaultvalue": "'sysdate()'"
	 }
     },
     "required": ["runDate"]
}
