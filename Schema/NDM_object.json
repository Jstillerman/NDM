{    "$schema": "http://json-schema.org/draft-06/schema#", 
     "id": "http://ndm.psfc.mit.edu/schemas/magnet_schema.json",
     "title": "_NDMobject",
     "description": "Information about database entry",
     "definitions": {
	 "LINK": {
	     "description": "link with link type pointing to record by rid. Internal to ODB.",
	     "type": "object",
	     "properties": {
		 "linked-class": {"type": "string"},
		 "rid": {"type": "string"}
	     },
	     "required": ["linked-class", "rid"]
	 },
	 "V": {
	     "description": "Vertex base class.  Internal to ODB.",
	     "type": "object",
	     "properties": {
		 "name": {"type": "string"},
		 "rid": {"type": "string"}
	     }
	 },
	 "ORestricted": {
	     "description": "Role management base class. Internal to ODB.",
	     "type": "object",
	     "properties": {
		 "name": {"type": "string"},
		 "type": {"type": "string"}
	     }
	 }
     },
     "type": "object",
     "allOf": [{ "$ref": "#/definitions/V" },{ "$ref": "#/definitions/ORestricted" }],
     "properties": {
	 "createdBy": {
	     "$ref": "#/definitions/LINK",
	     "defaultvalue": {"linked-class":"OUser","rid":"'getHTTPuser()'"}
	 },
	 "creationTime": {
	     "type": "string",
	     "format": "date-time",
	     "defaultvalue": "'sysdate'()"
	 },
	 "guid": {
	     "type": "string",
	     "defaultvalue" : "'uuid()'"
	 }
     },
     "required": ["creationTime", "createdBy"]
}
